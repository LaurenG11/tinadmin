        <SQL_FORM DB="eMart Data">
          <SQL_STMT_U val="SELECT DISTINCT a.Contact_Id,  a.Tag_id, a.Contact_Organization_Name, c.static_path, c.target_url FROM Contact AS a INNER JOIN Entity_Subscription AS b ON a.Contact_Id = b.Entity_ID inner join site_owner_defaults as c on a.contact_parent_id COLLATE Latin1_General_CI_AS = c.site_owner COLLATE Latin1_General_CI_AS WHERE  b.End_Date >= GETDATE() AND a.Contact_Organization_Name &lt;> '' AND a.contact_parent_id='1509386' AND a.Contact_Id COLLATE Latin1_General_CI_AS NOT IN (SELECT city_admin COLLATE Latin1_General_CI_AS FROM city) AND a.Contact_Id COLLATE Latin1_General_CI_AS NOT IN (SELECT region_admin COLLATE Latin1_General_CI_AS FROM region)  AND a.Contact_Id COLLATE Latin1_General_CI_AS NOT IN (SELECT country_admin COLLATE Latin1_General_CI_AS FROM country) AND a.Contact_Id COLLATE Latin1_General_CI_AS NOT IN (SELECT admin COLLATE Latin1_General_CI_AS FROM neighbourhood) AND a.Contact_Id&lt;>a.contact_parent_id&#xD;&#xA;"/>
          <OUTARG>
            <Arguments>
              <Argument Variable="1"/>
              <Argument Variable="2"/>
              <Argument Variable="3"/>
              <Argument Variable="4"/>
              <Argument Variable="5"/>
            </Arguments>
          </OUTARG>
        </SQL_FORM>
